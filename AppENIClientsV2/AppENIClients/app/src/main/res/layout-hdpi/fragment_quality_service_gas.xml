<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lo_encontrecom.appeniclients.QualityServiceGasFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/paddingHDPI"
        android:layout_gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/logo"
            android:layout_width="@dimen/imgWidthHDPI"
            android:layout_height="@dimen/imgHeighHDPI"
            android:layout_gravity="center"
            android:paddingTop="@dimen/nav_header_vertical_spacingHDPI"
            android:src="@drawable/logo_eni"/>

        <View
            android:layout_width="@dimen/lineWidthFragmentHDPI"
            android:layout_height="@dimen/lineHeighHDPI"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/lineMarginTopHDPI"
            android:layout_marginBottom="@dimen/lineMarginBottomHDPI"
            android:background="@color/ColorLine" />

        <TextView
            android:id="@+id/lblDeliveryTime"
            android:layout_marginTop="@dimen/marginHDPI"
            android:layout_width="wrap_content"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:text="@string/lblQualityService"
            android:textColor="@color/colorEstadoPedido"
            android:textSize="@dimen/textView_textSizeHDPI"
            android:textStyle="normal|bold" />

        <RatingBar
            android:id="@+id/ratingBar"
            android:layout_marginTop="@dimen/marginHDPI"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="5"
            android:stepSize="1.0"
            android:rating="2.0"
            android:theme="@style/RatingBar"/>

        <Button
            android:id="@+id/btnSend"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/imgMarginTopHDPI"
            android:background="@drawable/border_btn_lyrics"
            android:text="@string/btnSend"
            android:textSize="@dimen/btnSizeHDPI"
            android:textColor="@color/ColorTextTitle"
            android:textStyle="normal|bold" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"/>

    </LinearLayout>

</FrameLayout>