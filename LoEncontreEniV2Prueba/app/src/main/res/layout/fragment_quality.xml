<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lo_encontrecom.loencontreeniv2.QualityServiceFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/padding"
        android:orientation="vertical">

    <TextView
        android:id="@+id/lblDeliveryTime"
        android:layout_marginTop="@dimen/margin"
        android:layout_width="wrap_content"
        android:gravity="center"
        android:layout_height="wrap_content"
        android:text="@string/lblQualityService"
        android:textColor="@color/colorEstadoPedido"
        android:textSize="25sp"
        android:textStyle="normal|bold" />

        <RatingBar
            android:id="@+id/ratingBar"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="20dp"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:numStars="3"
            android:stepSize="1.0"
            android:rating="1.0" />

        <Button
            android:id="@+id/btnSend"
            android:layout_width="@dimen/width"
            android:layout_height="match_parent"
            android:layout_marginTop="35dp"
            android:background="@drawable/border_btn_lyrics"
            android:text="@string/btnSend"
            android:textSize="@dimen/btnSize"

            android:textColor="@color/colorPrimary"
            android:textStyle="normal|bold" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@null"/>



    </LinearLayout>

</FrameLayout>
